-      } else {
-        dataName.append(interestName);
-      }
-    } else {
-      NS_LOG_INFO("node(" << GetNode()->GetId() << ") received unwanted Interest: " << interest->getName());
-      return;
-    }
-  } else {
-    dataName.append(interest->getName());
-  }
-  // dataName.append(m_postfix);
-  // dataName.appendVersion();
