-TypeId
-ARProducer::GetTypeId(void)
-{
-  static TypeId tid =
-    TypeId("ns3::ndn::ARProducer")
-      .SetGroupName("Ndn")
-      .SetParent<App>()
-      .AddConstructor<ARProducer>()
-      .AddAttribute("Prefix", "Prefix, for which producer has the data", StringValue("/"),
-                    MakeNameAccessor(&ARProducer::m_prefix), MakeNameChecker())
-      .AddAttribute("ScheduleData", "Activate push data scheduling", BooleanValue(false),
-                    MakeBooleanAccessor(&ARProducer::m_scheduleData), MakeBooleanChecker())
-      .AddAttribute("ScheduleDataFrequency", "Frequency of data pushed", StringValue("1.0"),
-                    MakeDoubleAccessor(&ARProducer::m_scheduledDataFrequency), MakeDoubleChecker<double>())
-      .AddAttribute("ScheduledDataName", "Name for the data pushed", StringValue("/"),
-                    MakeNameAccessor(&ARProducer::m_scheduledDataName), MakeNameChecker())
-      .AddAttribute("ScheduledDataDestination", "Destination of the data pushed", StringValue("/"),
-                    MakeNameAccessor(&ARProducer::m_scheduledDataDestination), MakeNameChecker())
-      .AddAttribute("GenArData", "ActivateGenerationOfData", BooleanValue("false"),
-                    MakeBooleanAccessor(&ARProducer::m_genArData), MakeBooleanChecker())
-      .AddAttribute(
-         "Postfix",
-         "Postfix that is added to the output data (e.g., for adding producer-uniqueness)",
-         StringValue("/"), MakeNameAccessor(&ARProducer::m_postfix), MakeNameChecker())
-      .AddAttribute("PayloadSize", "Virtual payload size for Content packets", UintegerValue(1024),
-                    MakeUintegerAccessor(&ARProducer::m_virtualPayloadSize),
-                    MakeUintegerChecker<uint32_t>())
-      .AddAttribute("Freshness", "Freshness of data packets, if 0, then unlimited freshness",
-                    TimeValue(Seconds(0)), MakeTimeAccessor(&ARProducer::m_freshness),
-                    MakeTimeChecker())
-      .AddAttribute(
-         "Signature",
-         "Fake signature, 0 valid signature (default), other values application-specific",
-         UintegerValue(0), MakeUintegerAccessor(&ARProducer::m_signature),
-         MakeUintegerChecker<uint32_t>())
-      .AddAttribute("KeyLocator",
-                    "Name to be used for key locator.  If root, then key locator is not used",
-                    NameValue(), MakeNameAccessor(&ARProducer::m_keyLocator), MakeNameChecker());
-  return tid;
-}
