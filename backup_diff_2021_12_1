-control P4Function(inout standard_metadata_t std_meta) {
-    //Used to match namespace of the packet
-    bit<64> componentHash = 0;
-    bit<64> sensorValue = 0;
-
-    bit<64> accRoll = 0; 
-    bit<64> gyroRoll = 0; 
-
-
-    bit<64> rollQAngle = 0;
-    bit<64> rollQBias = 0;
-    bit<64> rollRMeasure = 0;
-
-
-    register<bit<64>>(4) rollP;
-    register<bit<64>>(1) rollAngleReg;
-    register<bit<64>>(1) rollBiasReg;
-
-    bit<64> rollAngle = 0; 
-
-
-
-    action loadSensorValues() {
-    }
-
-    action nop() {
-    }
-
-
-
-    action readAccValue() {
-        bit<64> accX;
-        bit<64> accY;
-        bit<64> accZ;
-        bit<64> aTanRollResult = 0;
-
-	//Calculate AccPitch and AccRoll 
-	//Acc Pitch
-
-	//Acc Roll 
-        log_msg("Loading the Accelerometer readings");
-        get_name_component_integer(accX, 4);
-        get_name_component_integer(accY, 5);
-        get_name_component_integer(accZ, 6);
-
-        log_msg("Calulating the Roll value using the Accelerometer Reading");
-        get_arc_tan(aTanRollResult, accY, accZ);
-	//converting to degrees by multiplying with radToDeg = 57.2957786 # 180/pi
-	//accRoll = ( aTanRollResult * 180 * 7 ) / 22; 
-	//Using the extern tan function itself to convert from radians to degrees as radian value is always rounded to 0 by p4 during int conversion 
-	accRoll = aTanRollResult; 
-	
-    }
-
-    action readGyroValue() {
-        bit<64> gyX = 0;
-        bit<64> gyY = 0;
-        bit<64> gyZ = 0;
-
-        log_msg("Loading the Gyroscope readings");
-        get_name_component_integer(gyX, 7);
-        get_name_component_integer(gyY, 8);
-        get_name_component_integer(gyZ, 9); 
-
-	gyroRoll = gyX * 57; 
-    }
-
-    action calculateKalmanRoll() {
-        log_msg("kalman 0");
-
-	bit<64> rate = 0;
-	//AK: update the dt value based on the sensor read frequency
-	bit<64> dt = 1;
-	bit<64> S = 1;
-
-
-        log_msg("kalman 0.1");
-
-    	bit<64> rollP00 = 0; 
-	bit<64> rollP01 = 0; 
-	bit<64> rollP10 = 0; 
-	bit<64> rollP11 = 0; 
-	bit<64> tempP00 = 0; 
-	bit<64> tempP01 = 0; 
-
-
-        log_msg("kalman 0.2");
-
-        rollP.read(rollP00, 0);
-        rollP.read(rollP01, 1);
-        rollP.read(rollP10, 2);
-        rollP.read(rollP11, 3);
-
-	bit<64> K0 = 0;
-	bit<64> K1 = 0;
-
-
-        log_msg("kalman 0.3");
-
-        rollAngleReg.read(rollAngle, 0);
-	
-    	bit<64> rollBias = 0;
-        rollBiasReg.read(rollBias, 0);
-
-    	bit<64> rollY = 0;
-
-        log_msg("kalman 1");
-
-	// step 1: 
-        rate = gyroRoll - rollBias;    // rate is the latest Gyro measurement
-        rollAngle = rollAngle + (dt * rate);
-
-        log_msg("kalman 2");
-        // Step 2:
-        rollP00 = rollP00 + ( dt * ((dt * rollP11) - rollP01 - rollP10 + rollQAngle));
-        rollP01 = rollP01 - (dt * rollP11);
-        rollP10 = rollP10 - (dt * rollP11);
-        rollP11 = rollP11 + (rollQBias * dt);
-
-
-        log_msg("kalman 3");
-
-        // Step 3
-        rollY = accRoll - rollAngle;
-
-        log_msg("kalman 4");
-        // Step 4: Innovation covariance
-        //S = rollP00 + rollRMeasure;
-	S = 1;
-
-
-        log_msg("kalman 5");
-        // Step 5:    Kalman Gain
-	get_int_division(K0, rollP00, S);
-	get_int_division(K1, rollP10, S);
-        //K0 = rollP00 / S;
-        //K1 = rollP10 / S;
-
-
-        log_msg("kalman 6");
-        // Step 6: Update the Angle
-        rollAngle = rollAngle + (K0 * rollY);
-        rollBias  = rollBias + (K1 * rollY);
-
-
-        rollAngleReg.write(0, rollAngle);
-        rollBiasReg.write(0, rollBias);
-
-        log_msg("kalman 7");
-        // Step 7: Calculate estimation error covariance - Update the error covariance
-        tempP00 = rollP00;
-        tempP01 = rollP01;
-
-	rollP00 = rollP00 - (K0 * tempP00);
-	rollP01 = rollP01 - (K0 * tempP01);
-	rollP10 = rollP10 - (K1 * tempP00);
-	rollP11 = rollP11 - (K1 * tempP01);
-
-        rollP.write(0, rollP00);
-        rollP.write(1, rollP01);
-        rollP.write(2, rollP10);
-        rollP.write(3, rollP11);
-
-    }
-
-    action updateValues() {
-	insert_component("result", 10);
-	insert_component_integer(rollAngle, 11);
-    }
-
-
-    action readSensorValues() {
-	readAccValue();
-	readGyroValue();
-	calculateKalmanRoll(); 
-	updateValues();
-    }
