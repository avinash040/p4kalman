-void
-ARProducer::ScheduleNextDataPush()
-{
-  if (m_firstTimeDataPushed) {
-    m_sendPushDataEvent = Simulator::Schedule(Seconds(0.0), &ARProducer::SendPushData, this);
-    m_firstTimeDataPushed = false;
-  }
-  else if (!m_sendPushDataEvent.IsRunning())
-    m_sendPushDataEvent = Simulator::Schedule(Seconds(1.0 / m_scheduledDataFrequency), &ARProducer::SendPushData, this);
-}
