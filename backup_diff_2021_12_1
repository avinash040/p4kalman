-void
-ARProducer::OnData(shared_ptr<const Data> data)
-{
-  if (!m_active)
-    return;
-
-  App::OnData(data); // tracing inside
-
-  NS_LOG_FUNCTION(this << data);
-
-
-  Name dataName;
-  if (data->getTag<lp::NameURITag>() != nullptr) {
-    //Handle topology name
-    const std::string& dataContentName = data->getTag<lp::SecondaryNameURITag>()->get();
-    if (dataContentName != "/") {
-      dataName.append(dataContentName);
-    } else {
-      dataName.append(data->getTag<lp::NameURITag>()->get());
-    }
-    if (!m_topologyName.isPrefixOf(dataName)) {
-      NS_LOG_INFO("node(" << GetNode()->GetId() << ") received unwanted Data: " << data->getName());
-      return;
-    }
-    Name command = dataName.getSubName(m_topologyName.size());
-    std::string commandString = command.at(0).toUri();
-    std::string commandParameter = command.at(1).toUri();
-    NS_LOG_INFO("ARProducer received the following command: " << commandString << ", param: " << commandParameter);
-    if (commandString == "change-rate") {
-      try {
-        m_scheduledDataFrequency = std::stod(commandParameter);
-      }
-      catch (std::invalid_argument& e) {
-        NS_LOG_INFO("The desired rate is not a number");
-      }
-    }
-    else {
-      NS_LOG_INFO("Unknown command: " << commandString);
-    }
-  } else {
-    dataName.append(data->getName());
-  }
-
-
-}
